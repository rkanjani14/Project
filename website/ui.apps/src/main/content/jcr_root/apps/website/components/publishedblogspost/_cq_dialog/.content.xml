<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
	xmlns:cq="http://www.day.com/jcr/cq/1.0"
	xmlns:jcr="http://www.jcp.org/jcr/1.0"
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
	jcr:primaryType="nt:unstructured" jcr:title="Properties"
	sling:resourceType="cq/gui/components/authoring/dialog">
	<content jcr:primaryType="nt:unstructured"
		sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
		<items jcr:primaryType="nt:unstructured">
			<column jcr:primaryType="nt:unstructured"
				sling:resourceType="granite/ui/components/coral/foundation/container">
				<items jcr:primaryType="nt:unstructured">




					<accordion jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
						composite="{Boolean}true" fieldLabel="Page List">

						<field jcr:primaryType="nt:unstructured"
							sling:resourceType="granite/ui/components/coral/foundation/container"
							name="./pagelist">
							<items jcr:primaryType="nt:unstructured">
								<column jcr:primaryType="nt:unstructured"
									sling:resourceType="granite/ui/components/coral/foundation/container">
									<items jcr:primaryType="nt:unstructured">
									
										<logo jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
											fieldLabel="Link to" name="./imagefield" />

										<text1 jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
											fieldLabel="typeofblog" name="./typeofblog" />


										<text2 jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
											fieldLabel="title" name="./title" />

										<titlelink jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
											fieldLabel="titlelink" name="./titlelink" />
											
										<text3 jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
											fieldLabel="description" name="./description" />

											
											<text4 jcr:primaryType="nt:unstructured"
											sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
											fieldLabel="Date" name="./date" />
											
											
									</items>
								</column>
							</items>
						</field>
					</accordion>

				</items>
			</column>
		</items>
	</content>
</jcr:root>